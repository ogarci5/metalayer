<h2 class="sub-header">Companies</h2>
<p>Showing <%=@companies.query[:pagination][:page_size]%> of <%=@companies.total%></p>
<div class="table-responsive">
  <table class="table table-striped">
    <thead>
      <tr>
        <th class="field-header" field="company_name">Company Name</th>
        <th class="field-header" field="city">City</th>
        <th class="field-header" field="state">State</th>
        <th class="field-header" field="industry">Industry</th>
        <th class="field-header" field="annual_revenue">Revenue</th>
        <th class="field-header" field="employees">Employees</th>
      </tr>
    </thead>
    <tbody id="companies">
      <%= render partial: 'table' %>
    </tbody>
  </table>
</div>
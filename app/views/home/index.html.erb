<h2 class="sub-header">Companies</h2>
<p>Showing <%=@companies.query[:pagination][:page_size]%> of <%=@companies.total%></p>
<div class="table-responsive">
  <table class="table table-striped">
    <thead>
      <tr>
        <th class="field-header" field="company_name"><a>Company Name</a></th>
        <th class="field-header" field="city"><a>City</a></th>
        <th class="field-header" field="state"><a>State</a></th>
        <th class="field-header" field="industry"><a>Industry</a></th>
        <th class="field-header" field="annual_revenue"><a>Revenue</a></th>
        <th class="field-header" field="employees"><a>Employees</a></th>
      </tr>
    </thead>
    <tbody id="companies">
      <%= render partial: 'table' %>
    </tbody>
  </table>
</div>